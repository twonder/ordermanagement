<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="MessageForwardingInCaseOfFaultConfig" type="NServiceBus.Config.MessageForwardingInCaseOfFaultConfig, NServiceBus.Core"/>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
    <section name="AuditConfig" type="NServiceBus.Config.AuditConfig, NServiceBus.Core"/>
    <section name="Logging" type="NServiceBus.Config.Logging, NServiceBus.Core"/>
  </configSections>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <Logging Threshold="Warn"/>
  <MessageForwardingInCaseOfFaultConfig ErrorQueue="consoleapp.error"/>
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Assembly="OrderEntry.Commands" Endpoint="OrderManagement.OrderEntry"/>
      <add Assembly="OrderProcessing.Commands" Endpoint="OrderManagement.OrderProcessing"/>
    </MessageEndpointMappings>
  </UnicastBusConfig>

  <AuditConfig QueueName="audit"/>
</configuration>